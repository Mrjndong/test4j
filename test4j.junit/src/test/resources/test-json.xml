<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="test4j">
	<test name="test4j">
		<groups>
			<run>
				<include name="json" />
				<exclude name="broken" />
				<exclude name="memery-db" />
				<exclude name="tracer" />
			</run>
		</groups>
		<packages>
			<package name="org.test4j.json.*" />
		</packages>
	</test>
</suite>