<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="test4j">
	<test name="test4j">
		<groups>
			<run>
				<include name="only-for-test" />
			</run>
		</groups>
		<packages>
			<package name="org.test4j.core.testng.testcase.*" />
		</packages>
	</test>
	<listeners>
		<listener class-name="org.test4j.core.testng.UserTestReporter" />
	</listeners>
</suite>