<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="test4j">
	<test name="test4j">
		<groups>
			<run>
				<exclude name="debug" />
			</run>
		</groups>
		<packages>
			<package name="org.test4j.spec.*"></package>
		</packages>
	</test>
	<listeners>
		<listener class-name="org.test4j.testng.report.UserTestReporter"></listener>
	</listeners>
</suite>